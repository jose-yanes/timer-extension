{
    "manifest_version" : 2,
    "name" : "Timer!",
    "version" : "1.0",

    "description" : "Timer for tracking pomodoro sessions.",

    "icons":{
        "48" : "icons/beasts-48.png"
    },

    "permissions" : ["activeTab"],

    "browser_action": {
        "default_icon" : "icons/beasts-32.png",
        "default_title" : "Pomodoro!",
        "default_popup" : "popup/pomodoro.html"
    }
}